<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ungpay.thirdpartyplatformsandframeworks">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.New">
        <activity android:name=".ui.alipaycardview.AlipayCardViewActivity"></activity>
        <activity android:name=".bluetooth.BlueToothActivity" />
        <activity android:name=".ui.DropDownMenu.DropDownMenuActivity" />
        <activity android:name=".eventsbus.EventBusSecondActivity" />
        <activity android:name=".eventsbus.EventsBusMainActivity" />
        <activity android:name=".ui.listview.ListViewGroups" />
        <activity android:name=".ui.SimpleExoPlayerTimeBar" />
        <activity
            android:name=".exoMedia.ExoMediaVideoPlayDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".exoMedia.ExoMediaAudioPlayDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />
        <activity android:name=".exoMedia.ExoMediaVideoPlayActivity" />
        <activity android:name=".exoMedia.ExoMediaAudioPlayActivity" />
        <activity android:name=".exoMedia.ExoMediaChooseActivity" />
        <activity android:name=".ui.customKeyBoard.CustomKeyboard" />
        <activity android:name=".ui.ViewLayoutTransation.ViewActivity" />
        <activity android:name=".ui.PhoneCard.EditTextWathcerActivity" />
        <activity android:name=".ui.fonts.FontsActivity" />
        <activity android:name=".ui.alipayui.AlipayHomeActivity" />
        <activity android:name=".ui.UiActivity" />
        <activity android:name=".OkGo.OkGoActivity" />
        <activity android:name=".mvp.view.MVPMainActivity" />
        <activity android:name=".okhttp.OkHttpActivity" />
        <activity android:name=".UmnegTongji.BugActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>